{
    "extends": ["react-app", "react-app/jest", "plugin:prettier/recommended"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": ["warn", { "singleQuote": true, "endOfLine": "auto", "printWidth": 100 }]
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "parser": "@typescript-eslint/parser"
}
